# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  olg_minecraft_config = debug
endif
ifeq ($(config),release)
  olg_minecraft_config = release
endif

PROJECTS := olg-minecraft

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

olg-minecraft:
ifneq (,$(olg_minecraft_config))
	@echo "==== Building olg-minecraft ($(olg_minecraft_config)) ===="
	@${MAKE} --no-print-directory -C . -f olg-minecraft.make config=$(olg_minecraft_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f olg-minecraft.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   olg-minecraft"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"